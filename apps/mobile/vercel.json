{
    "buildCommand": "pnpm install && pnpm build:web",
    "outputDirectory": "dist",
    "devCommand": "pnpm start:web",
    "cleanUrls": true,
    "framework": "nextjs",
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "/"
      }
    ],
    "installCommand": "pnpm install --frozen-lockfile",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ]
  }
  